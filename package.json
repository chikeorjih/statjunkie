{
  "name": "stat-junkie",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "hookrouter": "^1.2.3",
    "react": "^16.8.6",
    "react-bootstrap-table-next": "^3.1.5",
    "react-dev-utils": "^9.0.1",
    "react-dom": "^16.8.6",
    "react-scripts": "3.0.1"
  },
  "scripts": {
    "build:css": "postcss src/styles/app.css -o src/index.css",
    "watch:css": "postcss src/styles/app.css -o src/index.css -w",
    "start": "npm-run-all -p watch:css start-js",
    "start-js": "react-scripts start",
    "build-js": "react-scripts build",
    "build": "npm-run-all build:css build-js",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^9.6.1",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^6.1.3",
    "postcss-import": "^12.0.1",
    "postcss-preset-env": "^6.7.0"
  }
}
